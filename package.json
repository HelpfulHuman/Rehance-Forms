{
  "name": "rehance-forms",
  "version": "1.0.0",
  "description": "Form utilities and higher-order components for React",
  "main": "dist/index.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push origin master && git push origin master --tags",
    "clean": "rm -rf dist/",
    "build": "rollup -c",
    "watch": "npm run build -- --watch",
    "test": "mocha",
    "tdd": "npm run watch | npm run test -- --watch",
    "prepublish": "npm run clean && npm run build && npm run test"
  },
  "dependencies": {
    "react": "^16.1.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "rollup": "^0.51.8",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript2": "^0.8.1",
    "rollup-watch": "^4.3.1",
    "sinon": "^4.1.2",
    "typescript": "^2.6.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HelpfulHuman/Rehance-Forms.git"
  },
  "keywords": [
    "react",
    "enhancements",
    "hoc",
    "utilities",
    "form",
    "forms"
  ],
  "author": "Nick Glenn <nick@helpfulhuman.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HelpfulHuman/Rehance-Forms/issues"
  },
  "homepage": "https://github.com/HelpfulHuman/Rehance-Forms#readme"
}
