{
  "name": "rehance-forms",
  "version": "4.5.0",
  "description": "Form utilities for React",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "preversion": "npm run clean && npm run build && npm run lint && npm run test",
