    "rollup": "^0.51.8",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-typescript2": "^0.20.1",
    "tslib": "^1.9.3",
    "typescript": "^3.4.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HelpfulHuman/Rehance-Forms.git"
  },
